<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="pl.imgw.baltrad.dex.model.User" table="baltrad_users">
    <id column="user_id" name="id">
      <generator class="increment"/>
    </id>
    <property column="user_name" name="name"/>
    <property column="password" name="password"/>
    <property column="role" name="role"/>
    <property column="factory" name="factory"/>
    <property column="country" name="country"/>
    <property column="city" name="city"/>
    <property column="zip_code" name="zipCode"/>
    <property column="street" name="street"/>
    <property column="number" name="number"/>
    <property column="phone" name="phone"/>
    <property column="email" name="email"/>
    <property column="node_address" name="nodeAddress"/>
    <property column="local_directory" name="localDirectory"/>
  </class>
</hibernate-mapping>
