<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="pl.imgw.baltrad.dex.model.Subscription" table="baltrad_subscriptions">
    <id column="subscription_id" name="subscriptionID">
      <generator class="increment"/>
    </id>
    <property column="user_id" name="userID"/>
    <property column="data_channel_id" name="dataChannelID"/>
  </class>
</hibernate-mapping>
