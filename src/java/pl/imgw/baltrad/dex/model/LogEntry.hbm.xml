<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<!--
    Hibernate mapping file for log entry object.
-->

<hibernate-mapping>
    <class name="pl.imgw.baltrad.dex.model.LogEntry" table="baltrad_logs">
    <id column="log_entry_id" name="id">
      <generator class="increment"/>
    </id>
    <property column="log_entry_rank" name="rank"/>
    <property column="log_entry_date" name="date"/>
    <property column="log_entry_time" name="time"/>
    <property column="log_entry_text" name="text"/>
  </class>
</hibernate-mapping>
