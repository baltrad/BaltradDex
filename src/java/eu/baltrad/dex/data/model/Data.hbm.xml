<hibernate-mapping>
  <class name="eu.baltrad.dex.data.model.Data" table="dex_data">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property column="channel_name" name="channelName"/>
    <property column="path" name="path"/>
    <property column="date" name="date"/>
    <property column="time" name="time"/>
  </class>
</hibernate-mapping>
