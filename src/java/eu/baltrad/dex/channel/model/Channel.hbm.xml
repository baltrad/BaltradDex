<hibernate-mapping>
  <class name="eu.baltrad.dex.channel.model.Channel" table="dex_channels">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">channel_id_seq</param>
      </generator>
    </id>
    <property column="name" name="channelName"/>
    <property column="wmo_number" name="wmoNumber"/>
  </class>
</hibernate-mapping>
