<hibernate-mapping>
  <class name="eu.baltrad.dex.config.model.Configuration" table="dex_node_configuration">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">configuration_id_seq</param>
      </generator>
    </id>
    <property column="name" name="nodeName"/>
    <property column="type" name="nodeType"/>
    <property column="short_address" name="shortAddress"/>
    <property column="port" name="portNumber"/>
    <property column="org_name" name="orgName"/>
    <property column="org_address" name="orgAddress"/>
    <property column="time_zone" name="timeZone"/>
    <property column="temp_dir" name="tempDir"/>
    <property column="email" name="adminEmail"/>
  </class>
</hibernate-mapping>
