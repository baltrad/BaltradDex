<hibernate-mapping>
  <class name="eu.baltrad.dex.log.model.LogEntry" table="dex_messages">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">log_entry_id_seq</param>
      </generator>
    </id>
    <property column="date" name="date"/>
    <property column="time" name="time"/>
    <property column="type" name="type"/>
    <property column="message" name="message"/>
  </class>
</hibernate-mapping>
