<hibernate-mapping>
  <class name="eu.baltrad.dex.subscription.model.Subscription" table="dex_subscriptions">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">subscription_id_seq</param>
      </generator>
    </id>
    <property column="user_name" name="userName"/>
    <property column="channel_name" name="channelName"/>
    <property column="node_address" name="nodeAddress"/>
    <property column="operator_name" name="operatorName"/>
    <property column="type" name="type"/>
    <property column="selected" name="selected"/>
  </class>
</hibernate-mapping>