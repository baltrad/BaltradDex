<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Hibernate mapping file for auxiliary subscription table.
-->
<hibernate-mapping>
  <class name="eu.baltrad.dex.model.AuxSubscription" table="dex_aux_subscriptions">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">aux_subscription_id_seq</param>
      </generator>
    </id>
    <property column="user_id" name="userId"/>
    <property column="channel_id" name="channelId"/>
  </class>
</hibernate-mapping>
