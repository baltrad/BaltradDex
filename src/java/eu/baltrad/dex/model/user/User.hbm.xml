<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="eu.baltrad.dex.model.user.User" table="dex_users">
    <id column="id" name="id">
      <generator class="native">
        <!--param name="sequence">user_id_seq</param-->
      </generator>
    </id>
    <property column="name" name="name"/>
    <property column="password" name="password"/>
    <property column="ret_password" name="retPassword"/>
    <property column="role" name="role"/>
    <property column="factory" name="factory"/>
    <property column="country" name="country"/>
    <property column="city" name="city"/>
    <property column="city_code" name="cityCode"/>
    <property column="street" name="street"/>
    <property column="number" name="number"/>
    <property column="phone" name="phone"/>
    <property column="email" name="email"/>
    <property column="node_address" name="nodeAddress"/>
  </class>
</hibernate-mapping>
