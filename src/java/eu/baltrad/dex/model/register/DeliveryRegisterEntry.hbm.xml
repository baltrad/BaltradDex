<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Hibernate mapping file for delivery register entry object.
-->
<hibernate-mapping>
  <class name="eu.baltrad.dex.model.register.DeliveryRegisterEntry" table="dex_delivery_register">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">delivery_register_id_seq</param>
      </generator>
    </id>
    <property column="user_id" name="userId"/>
    <property column="data_id" name="dataId"/>
  </class>
</hibernate-mapping>
