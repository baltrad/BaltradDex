<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Hibernate mapping file for log entry object.
-->
<hibernate-mapping>
  <class name="eu.baltrad.dex.model.LogEntry" table="dex_messages">
    <id column="id" name="id">
      <generator class="sequence">
        <param name="sequence">log_entry_id_seq</param>
      </generator>
    </id>
    <property column="date" name="date"/>
    <property column="time" name="time"/>
    <property column="type" name="type"/>
    <property column="message" name="message"/>
  </class>
</hibernate-mapping>
